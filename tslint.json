{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
			"indent": [true, "tabs", 4],
			"semicolon": [true, "always"],
			"quotemark": [true, "single", "jsx-double"],
			"no-console": false,
			"no-var-requires": false,
			"trailing-comma": false,
			"no-debugger": false,
			"ordered-imports": false,
			"object-literal-sort-keys": false,
			"max-line-length": [true, 160]
    },
    "rulesDirectory": []
}
